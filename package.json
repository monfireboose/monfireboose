{
  "name": "monfireboose",
  "version": "0.1.4-experimental.1",
  "description": "A lightweight JavaScript library that provides a high level and model based API for interacting with Firebase.",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup index.ts --format cjs,esm --dts",
    "lint": "eslint . --ext .ts --ext .tsx",
    "test": "jest",
    "lint:fix": "eslint --fix . --ext .ts --ext .tsx",
    "prepare": "npm run build"
  },
  "dependencies": {
    "firebase": "^10.12.5",
    "pluralize": "^8.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/pluralize": "^0.0.33",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.7.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  },
  "keywords": [
    "firebase",
    "javascript",
    "library",
    "api",
    "interact",
    "data",
    "database",
    "authentication",
    "storage",
    "firestore",
    "cloud",
    "realtime",
    "cloud",
    "messaging",
    "analytics",
    "hosting",
    "functions"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/fireboose/fireboose.git"
  },
  "author": "Albert Arakelyan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/monfireboose/monfireboose/issues"
  },
  "homepage": "https://github.com/monfireboose/monfireboose#readme"
}
